<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.dietandcalorietracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".fitTestButton"
            android:label="Fit Test" />
        <activity
            android:name=".FitTestResult"
            android:label="Fit Test Results" />
        <activity
            android:name=".about_us"
            android:label="About Us">

        </activity>
        <activity
            android:name=".help_txt"
            android:label="Help">

        </activity>
        <activity android:name=".Search_food"
            android:label="Search for food">

        </activity>
    </application>

</manifest>